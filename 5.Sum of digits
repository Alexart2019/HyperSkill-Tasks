import java.util.Scanner;

class Main {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        int num = scanner.nextInt();
        int ost1 = num%10;
        int ost2 = (num/10)%10;
        int ost3 = num/100;

        System.out.println(ost1+ost2+ost3);
    }
}
